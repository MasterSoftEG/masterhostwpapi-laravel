{
    "name": "MasterSoftEG/masterhostwpapi-laravel",
    "description": "Laravel SDK for masterhosteg  with event dispatching and webhook route",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=9.0",
        "illuminate/http": ">=9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "masterhosteg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "masterhosteg\\masterhostegServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
